@article{pintodossantosMedicalStudentsAttitude2019,
  title = {Medical Students' Attitude towards Artificial Intelligence: A Multicentre Survey},
  shorttitle = {Medical Students' Attitude towards Artificial Intelligence},
  author = {{Pinto dos Santos}, D. and Giese, D. and Brodehl, S. and Chon, S. H. and Staab, W. and Kleinert, R. and Maintz, D. and Baeßler, B.},
