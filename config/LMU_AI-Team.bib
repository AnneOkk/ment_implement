@article{garavand_etal19,
  title = {Effective {{Factors}} in {{Adoption}} of {{Mobile Health Applications}} between {{Medical Sciences Students Using}} the {{UTAUT Model}}},
  author = {Garavand, Ali and Samadbeik, Mahnaz and Nadri, Hamed and Rahimi, Bahlol and Asadi, Heshmatollah},
