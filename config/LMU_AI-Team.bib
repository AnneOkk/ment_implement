@article{titovImprovingAdherenceClinical2013,
  title = {Improving {{Adherence}} and {{Clinical Outcomes}} in {{Self-Guided Internet Treatment}} for {{Anxiety}} and {{Depression}}: {{Randomised Controlled Trial}}},
  shorttitle = {Improving {{Adherence}} and {{Clinical Outcomes}} in {{Self-Guided Internet Treatment}} for {{Anxiety}} and {{Depression}}},
  author = {Titov, Nickolai and Dear, Blake F. and Johnston, Luke and Lorian, Carolyn and Zou, Judy and Wootton, Bethany and Spence, Jay and McEvoy, Peter M. and Rapee, Ronald M.},
  year = {2013},
