  author = {Tran, Tung and Kavuluru, Ramakanth},
  year = {2017},
  month = nov,
  journal = {Journal of Biomedical Informatics},
  series = {Supplement: {{A Natural Language Processing Challenge}} for {{Clinical Records}}: {{Research Domains Criteria}} ({{RDoC}}) for {{Psychiatry}}},
  volume = {75},
