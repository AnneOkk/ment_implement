@article{rennPreferenceInpersonPsychotherapy2019,
  title = {Preference for In-Person Psychotherapy versus Digital Psychotherapy Options for Depression: Survey of Adults in the {{U}}.{{S}}},
  shorttitle = {Preference for In-Person Psychotherapy versus Digital Psychotherapy Options for Depression},
  author = {Renn, Brenna N. and Hoeft, Theresa J. and Lee, Heather Sophia and Bauer, Amy M. and Areán, Patricia A.},
  year = {2019},
  month = feb,
  journal = {npj Digital Medicine},
  volume = {2},
  number = {1},
  pages = {1--7},
  publisher = {{Nature Publishing Group}},
  issn = {2398-6352},
  doi = {10.1038/s41746-019-0077-1},
  abstract = {Several barriers complicate access to psychotherapy for depression, including time commitment, location of services, and stigma. Digital treatment has the potential to address these barriers, yet long term use of digital psychotherapy is poor. This paper presents data from a mixed-methods, online survey to document concerns patients with depression face when given the choice of in-person psychotherapy and digital psychotherapy. Participants were 164 adults living in the United States who had previously used or considered psychotherapy for depression. Rural-dwelling and racial/ethnic minority (Native American, African American, and Spanish-speaking) respondents were purposively sampled. Participants were asked their preferences for and opinions about four treatment modalities: self-guided digital, peer-supported digital, expert-guided digital, or in-person psychotherapy. Less than half (44.5\%) of participants preferred in-person psychotherapy, 25.6\% preferred self-guided digital treatment, 19.7\% preferred expert-guided digital treatment, and 8.5\% peer-supported digital treatment. Principal themes extracted from qualitative analysis centered on the efficacy of digital treatment, access to digital treatment, concerns about peer-supported care, confidentiality and privacy concerns, preference for in-person treatment, skepticism about self-guided therapy, and the impact of social anxiety on the use of video-chat based care. Future development of digital psychotherapy will need to address concerns regarding efficacy, privacy, data security, and methods to enhance motivation to use these treatments.},
  copyright = {2019 The Author(s)},
  langid = {english},
  keywords = {Health services,Human behaviour,Therapeutics},
  file = {/Users/annekleine/Zotero/storage/ZL4MD6LE/Renn et al. - 2019 - Preference for in-person psychotherapy versus digi.pdf;/Users/annekleine/Zotero/storage/33WT9SZL/s41746-019-0077-1.html}
}

