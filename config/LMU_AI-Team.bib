  shorttitle = {Separating Depressive Comorbidity from Panic Disorder},
  author = {Lueken, Ulrike and Straube, Benjamin and Yang, Yunbo and Hahn, Tim and {Beesdo-Baum}, Katja and Wittchen, Hans-Ulrich and Konrad, Carsten and Ströhle, Andreas and Wittmann, André and Gerlach, Alexander L. and Pfleiderer, Bettina and Arolt, Volker and Kircher, Tilo},
  year = {2015},
