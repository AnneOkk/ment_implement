	year = {2016},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/aur.1615},
	keywords = {machine learning, autism spectrum disorder, eye tracking, face processing},
	pages = {888--898},
	annote = {https://doi-org.proxy-ub.rug.nl/10.1002/aur.1615},
