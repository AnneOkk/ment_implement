	month = dec,
	year = {2019},
	note = {Company: JMIR Mental Health
Distributor: JMIR Mental Health
Institution: JMIR Mental Health
Label: JMIR Mental Health
Publisher: JMIR Publications Inc., Toronto, Canada},
	pages = {e14108},
