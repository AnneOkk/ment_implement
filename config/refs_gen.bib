	journal = {Brain Structure and Function},
	author = {Liu, Feng and Guo, Wenbin and Fouche, Jean-Paul and Wang, Yifeng and Wang, Wenqin and Ding, Jurong and Zeng, Ling and Qiu, Changjian and Gong, Qiyong and Zhang, Wei and Chen, Huafu},
	month = jan,
	year = {2015},
	keywords = {Support vector machine, Consensus features, Functional connectivity, Multivariate pattern analysis, Resting-state fMRI, Social anxiety disorder/social phobia},
	pages = {101--115},
	file = {Full Text PDF:/Users/anne/Zotero/storage/43UCC3R6/Liu et al. - 2015 - Multivariate classification of social anxiety diso.pdf:application/pdf},
}

